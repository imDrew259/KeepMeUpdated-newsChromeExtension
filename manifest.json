{
    "name": "News-Extension",
    "version": "0.1",
    "description": "An extension to get daily News",
    "manifest_version": 2,
    "browser_action":{
        "default_icon": "newsicon.png", 
        "default_popup": "popup.html" 
    },
    "permissions": ["http://newsapi.org/v2/top-headlines"]
}
